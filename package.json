{
  "name": "@endless-interactive/components-library",
  "version": "0.0.5",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "check": "svelte-kit sync && svelte-check",
    "check:watch": "svelte-kit sync && svelte-check --watch",
    "lint": "prettier --plugin-search-dir components-library --check . && eslint .",
    "format": "prettier --plugin-search-dir components-library --write .",
    "publish:prod": "yarn build && npm publish ./package",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "next",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@vitest/coverage-c8": "^0.29.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.50.1",
    "svelte-check": "^2.7.1",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.6",
    "vite": "^4.0.0",
    "vitest": "^0.29.2"
  },
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
